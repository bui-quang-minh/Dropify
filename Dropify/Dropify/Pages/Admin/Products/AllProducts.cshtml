        <div class="page-wrapper">
            <div class="content container-fluid">

                <div class="page-header">
                    <div class="row">
                        <div class="col-sm-12">
                            <h3 class="page-title">Management Product</h3>
                            
                        </div>
                    </div>
                </div>
                <div class="row mb-5">
                    <div class="col">
                        <a class="btn btn-success" data-bs-toggle="modal" href="#add_modal">
                            <i class="fe fe-plus"></i>
                            New Product
                        </a>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="datatable table table-hover table-center mb-0">
                                        <thead>
                                            <tr>
                                                <th width="100">ProductID</th>
                                                <th width="300">Name</th>
                                                <th width="300">Categories</th>
                                                <th width ="300">Supplier</th>
                                                <th width="100">Original Price</th>
                                                <th width="100">Sell Out Price</th>
                                                <th width ="100">Status</th>
                                                <th class="text-end">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Model.ListProduct != null)
                                            {
                                                @foreach (var p in Model.ListProduct)
                                                {

                                                    <tr>
                                                        <td>
                                                            @p.ProductId
                                                        </td>
                                                        <td>
                                                            @p.Name
                                                        </td>
                                                        <td>@p.Category.CategoryName </td>
                                                        <td>
                                                            @p.Supplier.SupplierName
                                                           
                                                        </td>

                                                        <td>
                                                            @p.OriginalPrice
                                                        </td>
                                                        <td>
                                                            @p.SellOutPrice
                                                        </td>

                                                        <td>
                                                            @p.Status
                                                        </td>

                                                        <td class="text-end">
                                                            <div class="actions">
                                                                <a class="btn btn-sm bg-success-light" data-bs-toggle="modal" data-id="@p.ProductId"
                                                                data-name="@p.Name" data-cate="@p.CategoryId" data-supp="@p.SupplierId"
                                                                   data-oprice="@p.OriginalPrice" data-sprice="@p.SellOutPrice" data-create="@p.CreatedDate" data-end="@p.EndDate"
                                                                   data-start="@p.StartDate" data-ship="@p.Shipdate" data-status="@p.Status" 
                                                                data-bs-target="#edit_modal" href="#edit_modal">
                                                                    <i class="fe fe-pencil"></i> Edit
                                                                </a>
                                                                <a data-bs-toggle="modal" data-bs-target="#delete_modal" href="#delete_modal" data-id="@p.ProductId" class="btn btn-sm bg-danger-light">
                                                                    <i class="fe fe-trash"></i> Delete
                                                                </a>
                                                            </div>
                                                        </td>
                                                    </tr>


                                                }
                                            }

                                           
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="modal fade" id="delete_modal" role="dialog"  aria-hidden="true" >
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content"  >

                <div class="modal-body">
                    <form method="post" asp-page="/Admin/Products/AllProducts" asp-page-handler="Delete">
                        <div class="form-content p-2">
                            <h4 class="modal-title">Delete</h4>
                            <p class="mb-4">Are you sure want to delete?</p>
                            <button type="submit" id="MyLink" class="btn btn-primary" >Delete </button>
                            <input type="hidden" value="" id="p_id" name="p_id"  />
                            <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
                        </div>
                    </form>
                    
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="edit_modal" aria-hidden="true" role="dialog">
        <div class="modal-dialog modal-dialog-centered" style="width: 900px ; max-width:100%" role="document">
            <div class="modal-content" >
                <div class="modal-body">
                    <div class="form-content p-2">
                        <div class="modal-header border-0">
                            <h4 class="modal-title">Edit</h4>
                            <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">×</span>
                            </button>
                        </div>
                        <div class="card">
                            <div class="card-body">
                                <form method="post" asp-page-handler="Edit" asp-page="/Admin/Products/AllProducts" autocomplete="off">
                                    <div class="row">
                                        <button type="submit" class="bv-hidden-submit" style="display: none; width: 0px; height: 0px;"></button>
                                        <input class="form-control" id="edit_id" asp-for="product.ProductId" type="hidden">
                                        <div class="col-md-12">
                                            <div class="form-group mb-3">
                                                <label>Name</label>
                                                <input class="form-control" id="productName" asp-for="product.Name"  type="text">
                                            </div>
                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label> Category </label>
                                                <select class="form-control category-name" id="categories" asp-for="product.CategoryId" >
                                                    @if (Model.categories != null)
                                                    {
                                                        @foreach (var op in Model.categories)
                                                        {
                                                            <option value="@op.CategoryId">@op.CategoryName</option>
                                                        }
                                                    }


                                                </select>
                                            </div>
                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label> Supplier </label>
                                                <select class="form-control supplier-name" id="supplier" asp-for="product.SupplierId" >
                                                    @if (Model.categories != null)
                                                    {
                                                        @foreach (var s in Model.suppliers)
                                                        {
                                                            <option value="@s.SupplierId">@s.SupplierName</option>
                                                        }
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label> Original Price </label>
                                                <input class="form-control" asp-for="product.OriginalPrice" id="oPrice" type="text"  >
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label>Sell Out Price</label>
                                                <input class="form-control" asp-for="product.SellOutPrice" id="sPrice" type="text" >
                                            </div>
                                        </div>
                                       
                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label>Created Date</label>
                                                <input class="form-control" asp-for="product.CreatedDate" type="date" id="createDate">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label>Start Date</label>
                                                <input class="form-control" asp-for="product.StartDate" type="date"  id="startDate">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label>End Date</label>
                                                <input class="form-control" asp-for="product.EndDate" type="date" id="endDate">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label>Ship Date</label>
                                                <input class="form-control" asp-for="product.Shipdate" type="date"  id="shipDate">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group mb-3">
                                                <label> Status </label>
                                                <select class="form-control" asp-for="product.Status"  id="pStatus">
                                                    <option value="Active">Active</option>
                                                    <option value="Inactive">Inactive</option>
                                                </select>
                                            </div>
                                        </div>

                                        <div class="mt-4">
                                            <button class="btn btn-primary" name="form_submit" value="submit" type="submit"> Save </button>
                                            <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Cancel</button>
                                        </div>
                                    </div>

                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="add_modal" aria-hidden="true" role="dialog" >
